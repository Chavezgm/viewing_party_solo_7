<h2>Discover Movies</h2>

<%= button_to 'Top Rated Movies', "/users/#{@user.id}", method: :get  %>

<%= form_with(url: "/users/#{@user.id}", method: :get) do |form| %>
  <%= form.label :title%>
  <%= form.text_field :title, placeholder: "Search for movie" %>

  <%= form.submit "Search" %>
<% end %>
